<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Numero2Activity">

    <TextView
        android:id="@+id/num2_lbl_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/num2_lbl_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="@+id/num2_lbl_login"
        app:layout_constraintStart_toStartOf="@+id/num2_lbl_login"
        app:layout_constraintTop_toBottomOf="@+id/num2_lbl_login" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:onClick="retournerAuMain"
        android:text="Retourner"
        app:layout_constraintEnd_toEndOf="@+id/num2_lbl_password"
        app:layout_constraintStart_toStartOf="@+id/num2_lbl_password"
        app:layout_constraintTop_toBottomOf="@+id/num2_lbl_password" />
</androidx.constraintlayout.widget.ConstraintLayout>